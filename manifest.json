{
   "background": {
      "persistent": true,
      "scripts": [ "libs/utils.js", "libs/bg.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "libs/modal.console.css" ],
      "js": [ "libs/jquery.min.js", "libs/utils.js", "libs/modal.js" ],
      "matches": [ "http://www.appodeal.com/*", "https://www.appodeal.com/*", "https://apps.admob.com/*", "https://console.developers.google.com/*" ]
   }, {
      "js": [ "login.js" ],
      "matches": [ "http://www.appodeal.com/profile/*", "https://www.appodeal.com/profile/*", "http://www.appodeal.com/dashboard*", "https://www.appodeal.com/dashboard*", "http://www.appodeal.com/apps*", "https://www.appodeal.com/apps*" ]
   }, {
      "css": [ "libs/modal.css" ],
      "js": [ "adunits.js", "libs/progressBar.js", "libs/admob.js", "libs/modal.js" ],
      "matches": [ "https://apps.admob.com/*" ]
   } ],
   "description": "This extension will create and sync Appodeal adunits in your Admob account.",
   "icons": {
      "16": "icon.png",
      "32": "icon32.png",
      "64": "icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Appodeal",
   "permissions": [ "tabs", "cookies", "http://www.appodeal.com/*", "https://www.appodeal.com/*", "https://apps.admob.com/*", "https://console.developers.google.com/*", "storage", "webNavigation", "https://chrome.google.com/webstore/detail/appodeal/*" ],
   "short_name": "Appodeal Admob Sync",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "12.5"
}
